  "subject": [],
  "abstracts": [],
  "other_titles": [
    {
      "note": "",
      "title": "AN ACT relating to health care; requiring hospitals to publish certain information relating to the pricing of goods and services; prohibiting a hospital from collecting medical debt that accrued while the hospital was out of compliance with that requirement; prescribing a procedure for the correction of violations of that requirement; authorizing certain investigations, civil actions and the imposition of certain administrative sanctions; providing for the publication of certain information relating to violations; requiring certain health care entities to provide certain information before taking any action to collect a medical debt; providing a penalty; and providing other matters properly relating thereto."
    }
  ],
