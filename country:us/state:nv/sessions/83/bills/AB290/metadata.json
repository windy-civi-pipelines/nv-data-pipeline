  "legislative_session": "83",
  "identifier": "AB290",
  "title": "Revises provisions relating to prior authorization for medical or dental care under health insurance plans. (BDR\u00a057-861)",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [],
  "other_titles": [
    {
      "note": "",
      "title": "AN ACT relating to insurance; imposing requirements governing prior authorization for medical or dental care; prohibiting an insurer from requiring prior authorization for covered emergency services or denying coverage for covered, medically necessary emergency services; requiring an insurer to publish certain information relating to requests for prior authorization on the Internet; requiring an insurer and the Commissioner of Insurance to compile certain reports; and providing other matters properly relating thereto."
    }
  ],
