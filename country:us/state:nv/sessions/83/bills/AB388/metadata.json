  "subject": [],
  "abstracts": [],
  "other_titles": [
    {
      "note": "",
      "title": "AN ACT relating to employment; requiring certain public and private employers to establish certain procedures and forms relating to paid family leave; requiring the Human Resources Commission and Labor Commissioner to establish certain regulations relating to paid family leave; requiring certain public and private employers to provide notice relating to paid family leave; revising provisions relating to paid family leave for certain state employees and employees of private employers; repealing certain provisions relating to leave; providing penalties; and providing other matters properly relating thereto."
    }
  ],
