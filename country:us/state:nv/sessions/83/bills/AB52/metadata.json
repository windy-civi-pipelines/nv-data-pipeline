  "legislative_session": "83",
  "identifier": "AB52",
  "title": "Revises provisions relating to the payment of claims under policies of health insurance. (BDR\u00a057-367)",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [],
  "other_titles": [
    {
      "note": "",
      "title": "AN ACT relating to insurance; requiring the Commissioner of Insurance to establish programs to inform providers of health care and insureds under health insurance policies \nof certain information relating to the payment of claims; revising provisions governing the payment of claims under policies of health insurance; establishing certain administrative penalties; requiring a health carrier to provide certain information to participating providers of health care and covered persons; requiring a health carrier to establish certain procedures for challenging the denial of a claim; and providing other matters properly relating thereto."
    }
  ],
