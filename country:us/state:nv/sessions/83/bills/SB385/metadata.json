  "identifier": "SB385",
  "title": "Revises provisions relating to certain transferable tax credits and certain tax abatements. (BDR\u00a032-826)",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [],
  "other_titles": [
    {
      "note": "",
      "title": "AN ACT relating to taxation; requiring applicants for certain transferable tax credits and certain tax abatements to agree to a workforce development plan; prescribing the requirements for a program of workforce development implemented under the plan; authorizing the Office of Economic Development to investigate the recipient of certain transferable tax credits and certain tax abatements for compliance with a workforce development plan; requiring the recipient of certain transferable tax credits and certain tax abatements to repay the amount of the credits and abatements if the recipient is not in substantial compliance with the workforce development plan; and providing other matters properly relating thereto."
    }
  ],
