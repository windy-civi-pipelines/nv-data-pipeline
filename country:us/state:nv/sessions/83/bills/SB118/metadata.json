  ],
  "subject": [],
  "abstracts": [],
  "other_titles": [
    {
      "note": "",
      "title": "AN ACT relating to Medicaid; requiring Medicaid to include coverage for certain services provided by a pharmacist; imposing requirements relating to the rate of reimbursement that a pharmacist must receive for services covered under Medicaid; prohibiting Medicaid or a managed care organization that provides health care services to recipients of Medicaid from requiring prior authorization for the services of a pharmacist under certain circumstances; and providing other matters properly relating thereto."
    }
  ],
