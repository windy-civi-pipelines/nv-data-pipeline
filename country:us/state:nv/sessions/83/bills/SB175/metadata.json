  "identifier": "SB175",
  "title": "Revises provisions governing the treatment of certain military credentials in the licensure and hiring of educational personnel. (BDR\u00a034-663)",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
