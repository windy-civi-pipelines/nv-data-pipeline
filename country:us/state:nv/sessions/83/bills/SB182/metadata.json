{
  "legislative_session": "83",
  "identifier": "SB182",
  "title": "Revises provisions relating to the staffing of health care facilities. (BDR\u00a040-33)",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [],
  "other_titles": [
    {
      "note": "",
      "title": "AN ACT relating to health care; requiring certain hospitals to establish staffing committees for technical and service staff; establishing requirements governing the staffing of certain health care facilities; requiring certain hospitals to keep certain records relating to staffing; requiring certain hospitals to report certain information relating to staffing; prohibiting certain health care facilities from taking certain retaliatory actions; providing for certain actions to investigate and correct certain violations relating to staffing; providing administrative penalties; and providing other matters properly relating thereto."
    }
  ],
