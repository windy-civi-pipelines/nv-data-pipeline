{
  "legislative_session": "83",
  "identifier": "AB191",
  "title": "Revises provisions relating to collective bargaining. (BDR\u00a023-889)",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [],
  "abstracts": [],
  "other_titles": [
    {
      "note": "",
      "title": "AN ACT relating to collective bargaining; authorizing collective bargaining for certain state employees; providing for the recognition of professional organizations; providing for the establishment of bargaining units and the designation of exclusive representatives; establishing certain rights for professional organizations and professional employees; establishing procedures for collective bargaining and for making collective bargaining agreements; authorizing the Government Employee-Management Relations Board to collect certain fees; prohibiting certain practices relating to collective bargaining; authorizing a state professional employer to use the services of the Division of Human Resource Management of the Department of Administration and the Attorney General for certain purposes; authorizing the Board of Regents of the University of Nevada to conduct collective bargaining negotiations and enter into collective bargaining agreements with certain employees; providing a civil penalty; and providing other matters properly relating thereto."
    }
  ],
